  <DataSets>
    <DataSet Name="BMtriffDataSet_DataTable_EN">
      <Query>
        <DataSourceName>DataBaseConnectionString</DataSourceName>
        <CommandText>SELECT         a.ApplicantNameEn AS 公司, a.AddrEn AS 地址, a.FAX AS 傳真, 
                          t.CountryEn AS 國別, k.KindEn AS 性質, CAST(c.SN AS int) AS SN, 
                          c.ProcedureName_EN AS 程序, c.Examtime AS [審查時間(月)], 
                          c.QuotaTotal AS [費用(含規費及複代理費)], c.Remark_EN AS 備註, 
                          t.SN AS CountrySN, k.SN AS KindSN, p.PhaseNameEN AS Phase, c.MeFee, 
                          c.GovFeeNT, c.AttorneyFeeNT, c.GovFeeNT + c.AttorneyFeeNT AS ForeginFee, 
                          c.LargeEntity, CONVERT(varchar, c.LargeEntity) 
                          + t.Country + k.Kind + c.Phase AS charLargeEntity
FROM             ClientFeeT_Temp AS c INNER JOIN
                          ApplicantT AS a ON c.ApplicantKey = a.ApplicantKey INNER JOIN
                          CountryT AS t ON c.Country = t.CountrySymbol INNER JOIN
                          KindT AS k ON c.Kind = k.KindSN INNER JOIN
                          PhaseT AS p ON c.Phase = p.PhaseKEY
WHERE         (c.ApplicantKey = @ApplicantNo) AND (c.ApplicantMode = @ApplicantMode)
ORDER BY  CountrySN, KindSN, SN</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
