  <DataSets>
    <DataSet Name="BMtriffDataSet_DataTable1">
      <Query>
        <DataSourceName>DataBaseConnectionString</DataSourceName>
        <CommandText>SELECT         a.ApplicantName AS 公司, a.Addr AS 地址, a.FAX AS 傳真, t.Country AS 國別, 
                          k.Kind AS 性質, CAST(c.SN AS int) AS SN, c.ProcedureName AS 程序, 
                          c.Examtime AS [審查時間(月)], c.QuotaTotal AS [費用(含規費及複代理費)], 
                          c.Remark AS 備註, t.SN AS CountrySN, k.SN AS KindSN, 
                          p.PhaseName AS Phase, c.MeFee, c.GovFeeNT, c.AttorneyFeeNT, 
                          c.GovFeeNT + c.AttorneyFeeNT AS ForeginFee, c.LargeEntity
FROM             ClientFeeT_Temp AS c INNER JOIN
                          ApplicantT AS a ON c.ApplicantKey = a.ApplicantKey INNER JOIN
                          CountryT AS t ON c.Country = t.CountrySymbol INNER JOIN
                          KindT AS k ON c.Kind = k.KindSN INNER JOIN
                          PhaseT AS p ON c.Phase = p.PhaseKEY
WHERE         (c.ApplicantKey = @ApplicantNo)
ORDER BY  CountrySN, k.KindSN</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
